            Location="https://idp5.example.org/idp/profile/Shibboleth/SSO"/>
        <SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
            Location="https://idp5.example.org/idp/profile/SAML2/POST/SSO"/>
